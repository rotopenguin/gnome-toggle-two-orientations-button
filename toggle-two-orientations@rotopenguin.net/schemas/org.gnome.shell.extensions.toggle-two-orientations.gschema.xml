<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="org.gnome.shell.extensions.toggle-two-orientations"
    path="/org/gnome/shell/extensions/toggle-two-orientations/">
    <key type="b" name="orientation-a">
      <default>false</default>
      <description>Flip the "normal" orientation the other way.</description>
    </key>
    <key type="b" name="orientation-b">
      <default>false</default>
      <description>Flip "landscape" the other way round.</description>
    </key>
  </schema>
</schemalist>
